{
    "name": "wcadena/gestor-inventarios-it",
    "type": "project",
    "description": "Gestor de Inventarios de area de Sistemas.",
    "homepage": "https://ecuatask.com/",
    "keywords": [
        "framework",
        "laravel",
        "css",
        "js",
        "less",
        "responsive",
        "back-end",
        "web",
        "admin"
    ],
    "authors": [
        {
            "name": "Wagner Alexander Cadena Lastra",
            "email": "wcadena@outlook.com"
        }
    ],
    "support": {
        "issues": "https://github.com/wcadena/gestor-inventarios-it/issues"
    },
    "license": "EPL-1.0",
    "require": {
        "php": "^7.2",
        "barryvdh/laravel-cors": "^0.11.4",
        "barryvdh/laravel-dompdf": "^0.8.5",
        "doctrine/dbal": "^2.9",
        "fideloper/proxy": "^4.0",
        "guzzlehttp/guzzle": "^6.3",
        "intervention/image": "^2.5",
        "laracasts/flash": "^3.0",
        "laravel/framework": "^6.2",
        "laravel/passport": "^7.5",
        "laravel/tinker": "^1.0",
        "laravelcollective/html": "^6.0",
        "maatwebsite/excel": "^3.1",
        "nesbot/carbon": "^2.25",
        "rachidlaasri/laravel-installer": "^4.0",
        "santigarcor/laratrust": "^5.2",
        "simplesoftwareio/simple-qrcode": "^2.0",
        "spatie/laravel-fractal": "^5.6",
        "spatie/laravel-menu": "^3.4",
        "webpatser/laravel-uuid": "^3.0",
        "wuwx/laravel-autonumber": "^3.1"
    },
    "require-dev": {
        "facade/ignition": "^1.4",
        "fzaninotto/faker": "^1.4",
        "mockery/mockery": "^1.0",
        "nunomaduro/collision": "^3.0",
        "phpunit/phpunit": "^8.0"
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/"
        },
        "classmap": [
            "database/seeds",
            "database/factories"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ]
    }
}
