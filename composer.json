{
    "name": "wcadena/gestor-inventarios-it",
    "type": "project",
    "description": "Gestor de Inventarios de area de Sistemas.",
    "homepage": "https://ecuatask.com/",
    "keywords": [
        "framework",
        "laravel",
        "css",
        "js",
        "less",
        "responsive",
        "back-end",
        "web",
        "admin"
    ],
    "authors": [
        {
            "name": "Wagner Alexander Cadena Lastra",
            "email": "wcadena@outlook.com"
        }
    ],
    "support": {
        "issues": "https://github.com/wcadena/gestor-inventarios-it/issues"
    },
    "license": "EPL-1.0",
    "require": {
        "php": "^7.3|^8.0",
        "barryvdh/laravel-dompdf": "^0.8.7",
        "doctrine/dbal": "^2.12",
        "fideloper/proxy": "^4.4",
        "fruitcake/laravel-cors": "^2.0",
        "guzzlehttp/guzzle": "^7.0.1",
        "intervention/image": "^2.5",
        "laracasts/flash": "^3.2",
        "laravel/framework": "^8.12",
        "laravel/passport": "^10.0",
        "laravel/tinker": "^2.5",
        "laravel/ui": "^3.0",
        "laravelcollective/html": "^6.2",
        "maatwebsite/excel": "^3.1",
        "nesbot/carbon": "^2.41",
        "rachidlaasri/laravel-installer": "^4.1",
        "santigarcor/laratrust": "^6.3",
        "simplesoftwareio/simple-qrcode": "^4.1",
        "spatie/laravel-fractal": "^5.8",
        "spatie/laravel-menu": "^3.6",
        "webpatser/laravel-uuid": "^3.0"
    },
    "require-dev": {
        "facade/ignition": "^2.5",
        "fakerphp/faker": "^1.9.1",
        "mockery/mockery": "^1.4.2",
        "nunomaduro/collision": "^5.0",
        "phpunit/phpunit": "^9.3.3",
        "wnx/laravel-stats": "^2.5"
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ]
    }
}
